# A GraphQL schema provides a root type for each kind of operation.
schema {
    # The query root.
    query: Query
}

type Query {
    userCurrent: User
}

type User {
    username: String!
    email: String!
    orders: [Order!]! @dataFetcherResult
}

type Order {
    number: String!
    price: String!
}

directive @dataFetcherResult on FIELD_DEFINITION
